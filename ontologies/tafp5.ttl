@prefix : <http://cdm.ovh/taf/tafp5#> .
@prefix bag: <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#> .
@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix taf: <http://data.europa.eu/949/> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix list: <http://www.ontologydesignpatterns.org/cp/owl/list.owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix tafp5: <https://cdm.ovh/taf/tafp5#> .
@prefix rst_typ: <http://cdm.ovh/rsm/rolling_stock/typology/typology#> .
@prefix rst_cons: <http://cdm.ovh/rsm/rolling_stock/consist/RollingStockConsist#> .
@base <http://cdm.ovh/taf/tafp5#> .

<http://cdm.ovh/taf/tafp5#> rdf:type owl:Ontology ;
                             owl:versionIRI <http://cdm.ovh/taf/0.1/tafp5#> ;
                             owl:imports <http://cdm.ovh/rsm/rolling_tock/typology> ;
                             rdfs:comment "In the context of operations in general, and the TAF TSI in particular, \"weight\"  is often used instead of \"mass\" while the latter is correct (and units are always mass units such as kg). In the present ontology, we only use \"mass\"  in identifiers and labels."@en .

#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#boolean
xsd:boolean rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype ;
         rdfs:comment "Not part of OWL2 datatype map. Will crash HermiT, not however Pellet."@en .


###  http://www.w3.org/2001/XMLSchema#dateTimeStamp
xsd:dateTimeStamp rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#decimal
xsd:decimal rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#string
xsd:string rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#time
xsd:time rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://www.ontologydesignpatterns.org/cp/owl/bag.owl#hasItem
bag:hasItem rdf:type owl:ObjectProperty .


###  http://www.ontologydesignpatterns.org/cp/owl/list.owl#nextItem
list:nextItem rdf:type owl:ObjectProperty .


###  http://www.ontologydesignpatterns.org/cp/owl/list.owl#previousItem
list:previousItem rdf:type owl:ObjectProperty .


###  http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasLocation
dul:hasLocation rdf:type owl:ObjectProperty ;
                rdfs:comment "To be understood as \"at some point in time\". Which point (or interval) will be given by the States."@en .


###  http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasTimeInterval
dul:hasTimeInterval rdf:type owl:ObjectProperty .


###  http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isClassifiedBy
dul:isClassifiedBy rdf:type owl:ObjectProperty .


###  http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isLocationOf
dul:isLocationOf rdf:type owl:ObjectProperty .


###  http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isSettingFor
dul:isSettingFor rdf:type owl:ObjectProperty .


###  http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#playsRole
dul:playsRole rdf:type owl:ObjectProperty .


###  http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#realizes
dul:realizes rdf:type owl:ObjectProperty .


###  http://www.w3.org/2004/02/skos/core#inScheme
skos:inScheme rdf:type owl:ObjectProperty .


###  http://www.w3.org/ns/prov#wasAttributedTo
prov:wasAttributedTo rdf:type owl:ObjectProperty ;
                     rdfs:domain prov:Entity .


###  http://www.w3.org/ns/prov#wasRevisionOf
prov:wasRevisionOf rdf:type owl:ObjectProperty .


###  https://cdm.ovh/taf/tafp5#atOperationalLocation
tafp5:atOperationalLocation rdf:type owl:ObjectProperty ;
                            rdfs:subPropertyOf dul:hasLocation ;
                            owl:inverseOf tafp5:isOperationalLocationOf ;
                            rdfs:domain tafp5:TrainLocationState ,
                                        [ rdf:type owl:Class ;
                                          owl:unionOf ( tafp5:Facility
                                                        tafp5:Track
                                                      )
                                        ] ;
                            rdfs:range tafp5:OperationalLocation ;
                            rdfs:comment "For time-independent locations (e.g. of Facilities)"@en ;
                            rdfs:label "at operational location"^^xsd:language .


###  https://cdm.ovh/taf/tafp5#atTimeInstant
tafp5:atTimeInstant rdf:type owl:ObjectProperty ;
                    rdfs:domain tafp5:OperationalState ;
                    rdfs:range time:Instant ;
                    rdfs:label "at time instant"@en .


###  https://cdm.ovh/taf/tafp5#hasComposition
tafp5:hasComposition rdf:type owl:ObjectProperty ,
                              owl:FunctionalProperty ;
                     rdfs:domain tafp5:TrainCompositionState ;
                     rdfs:range rst_typ:RollingStock ;
                     rdfs:label "has composition"@en .


###  https://cdm.ovh/taf/tafp5#hasDestination
tafp5:hasDestination rdf:type owl:ObjectProperty ,
                              owl:FunctionalProperty ;
                     rdfs:domain tafp5:JourneySchedule ;
                     rdfs:range tafp5:OperationalLocation ;
                     rdfs:label "has destination"@en .


###  https://cdm.ovh/taf/tafp5#hasFacilityFunction
tafp5:hasFacilityFunction rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf dul:isClassifiedBy ;
                          rdfs:domain tafp5:Facility ;
                          rdfs:range tafp5:FacilityRoleOrFunction ;
                          rdfs:label "has facility function"@en .


###  https://cdm.ovh/taf/tafp5#hasJourney
tafp5:hasJourney rdf:type owl:ObjectProperty ;
                 rdfs:domain tafp5:TrainJourneyState ;
                 rdfs:range tafp5:Journey ;
                 rdfs:comment "not a functional property, as it can be expressed w.r.t. the planned journey or the executed journey (if available). User feedback welcome."@en ;
                 rdfs:label "has journey" .


###  https://cdm.ovh/taf/tafp5#hasJourneySchedule
tafp5:hasJourneySchedule rdf:type owl:ObjectProperty ;
                         rdfs:domain tafp5:Journey ;
                         rdfs:range tafp5:JourneySchedule ;
                         rdfs:comment "Note: it may be that the same path or planned journey has multiple schedules (variants), not further characterized here, so we did not make this property functional."@en ;
                         rdfs:label "has journey schedule" .


###  https://cdm.ovh/taf/tafp5#hasJourneySection
tafp5:hasJourneySection rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf bag:hasItem ;
                        owl:inverseOf tafp5:inJourneySchedule ;
                        rdf:type owl:InverseFunctionalProperty ;
                        rdfs:domain tafp5:JourneySchedule ;
                        rdfs:range tafp5:JourneySection ;
                        rdfs:comment "Inverse functional property."@en ;
                        rdfs:label "has journey section" .


###  https://cdm.ovh/taf/tafp5#hasOTN
tafp5:hasOTN rdf:type owl:ObjectProperty ;
             rdfs:domain tafp5:TrainRun ;
             rdfs:range tafp5:OTN ;
             rdfs:comment "The same train run can have different Operational Train Numbers in its course, depending on the operator or network over which it is run."@en ;
             rdfs:label "has OTN"@en ;
             rdfs:seeAlso "http://data.europa.eu/949/hasOperationalTrainNumberIdentifier"^^xsd:anyURI .


###  https://cdm.ovh/taf/tafp5#hasOrigin
tafp5:hasOrigin rdf:type owl:ObjectProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain tafp5:JourneySchedule ;
                rdfs:range tafp5:OperationalLocation ;
                rdfs:label "has origin"@en .


###  https://cdm.ovh/taf/tafp5#hasPayload
tafp5:hasPayload rdf:type owl:ObjectProperty ;
                 rdfs:domain tafp5:TransportCollaborationMessage ;
                 rdfs:range tafp5:OperationalState ;
                 rdfs:label "has payload"@en .


###  https://cdm.ovh/taf/tafp5#hasPrimaryCode
tafp5:hasPrimaryCode rdf:type owl:ObjectProperty ,
                              owl:FunctionalProperty ;
                     rdfs:domain tafp5:OperationalLocation ;
                     rdfs:range tafp5:LocationPrimaryCode ;
                     rdfs:label "has primary code"@en .


###  https://cdm.ovh/taf/tafp5#hasReferenceObject
tafp5:hasReferenceObject rdf:type owl:ObjectProperty ;
                         rdfs:domain tafp5:TransportCollaborationMessage ;
                         rdfs:range [ rdf:type owl:Class ;
                                      owl:unionOf ( tafp5:ITU
                                                    tafp5:TrainRun
                                                    tafp5:Wagon
                                                  )
                                    ] ;
                         rdfs:label "has reference object" .


###  https://cdm.ovh/taf/tafp5#hasSubsidiaryCode
tafp5:hasSubsidiaryCode rdf:type owl:ObjectProperty ,
                                 owl:FunctionalProperty ;
                        rdfs:domain tafp5:OperationalLocation ;
                        rdfs:range tafp5:LocationSubsidiaryCode ;
                        rdfs:label "has subsidiary code"@en .


###  https://cdm.ovh/taf/tafp5#hasTimeExtent
tafp5:hasTimeExtent rdf:type owl:ObjectProperty ;
                    rdfs:domain tafp5:OperationalState ;
                    rdfs:range time:TemporalEntity ;
                    rdfs:label "has time extent" .


###  https://cdm.ovh/taf/tafp5#hasTrain
tafp5:hasTrain rdf:type owl:ObjectProperty ,
                        owl:FunctionalProperty ;
               rdfs:domain tafp5:TrainRun ;
               rdfs:range tafp5:Train ;
               rdfs:label "has train"@en .


###  https://cdm.ovh/taf/tafp5#hasTrainRunState
tafp5:hasTrainRunState rdf:type owl:ObjectProperty ,
                                owl:InverseFunctionalProperty ;
                       rdfs:label "has train run state" .


###  https://cdm.ovh/taf/tafp5#hasTrainState
tafp5:hasTrainState rdf:type owl:ObjectProperty ,
                             owl:InverseFunctionalProperty ;
                    rdfs:domain tafp5:Train ;
                    rdfs:range tafp5:TrainState ;
                    rdfs:label "has train state"@en .


###  https://cdm.ovh/taf/tafp5#inJourneySchedule
tafp5:inJourneySchedule rdf:type owl:ObjectProperty ;
                        rdfs:label "in journey schedule"@en .


###  https://cdm.ovh/taf/tafp5#isOperationalLocationOf
tafp5:isOperationalLocationOf rdf:type owl:ObjectProperty ;
                              rdfs:subPropertyOf dul:isLocationOf ;
                              rdfs:label "is operational location of"@en .


###  https://cdm.ovh/taf/tafp5#isStateOfTrainRun
tafp5:isStateOfTrainRun rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf dul:isSettingFor ;
                        rdf:type owl:FunctionalProperty ;
                        rdfs:domain tafp5:TrainRunState ;
                        rdfs:range tafp5:TrainRun ;
                        rdfs:label "is state of train run" .

[ owl:inverseOf tafp5:isStateOfTrainRun ;
   rdfs:subPropertyOf tafp5:hasTrainRunState
 ] .


###  https://cdm.ovh/taf/tafp5#nextDescriptionVersion
tafp5:nextDescriptionVersion rdf:type owl:ObjectProperty ;
                             owl:inverseOf tafp5:previousDescriptionVersion ;
                             rdf:type owl:FunctionalProperty ,
                                      owl:InverseFunctionalProperty ;
                             rdfs:label "next description version" .


###  https://cdm.ovh/taf/tafp5#nextSection
tafp5:nextSection rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf list:nextItem ;
                  rdf:type owl:FunctionalProperty ,
                           owl:InverseFunctionalProperty ;
                  rdfs:domain tafp5:JourneySection ;
                  rdfs:range tafp5:JourneySection ;
                  rdfs:label "next section"@en .


###  https://cdm.ovh/taf/tafp5#playsFacilityRole
tafp5:playsFacilityRole rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf dul:playsRole ;
                        rdfs:domain tafp5:Facility ;
                        rdfs:range tafp5:FacilityRoleOrFunction ;
                        rdfs:label "plays facility role"@en .


###  https://cdm.ovh/taf/tafp5#playsOperationalRole
tafp5:playsOperationalRole rdf:type owl:ObjectProperty ;
                           rdfs:subPropertyOf dul:playsRole ;
                           rdfs:domain dul:Organization ;
                           rdfs:range tafp5:OperationalRole ;
                           rdfs:label "plays operational role"@en .


###  https://cdm.ovh/taf/tafp5#previousDescriptionVersion
tafp5:previousDescriptionVersion rdf:type owl:ObjectProperty ;
                                 rdfs:subPropertyOf prov:wasRevisionOf ,
                                                    tafp5:revisesDescriptionVersion ;
                                 rdf:type owl:FunctionalProperty ,
                                          owl:InverseFunctionalProperty ;
                                 rdfs:domain tafp5:VersionedDescription ;
                                 rdfs:range tafp5:VersionedDescription ;
                                 rdfs:comment "A transitive property linking a new description to the (sole) previous one it revises. \"Supersedes\" would be a ternary relation, the value of which could be deducted from the \"effective from\" datetimes, and the datetime of the query: not conceptualized in this ontology."@en ;
                                 rdfs:label "previous description version" .


###  https://cdm.ovh/taf/tafp5#previousSection
tafp5:previousSection rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf list:previousItem ;
                      rdf:type owl:FunctionalProperty ,
                               owl:InverseFunctionalProperty ;
                      rdfs:domain tafp5:JourneySection ;
                      rdfs:range tafp5:JourneySection ;
                      rdfs:label "previous section"@en .


###  https://cdm.ovh/taf/tafp5#realizesPlannedJourneyOrPath
tafp5:realizesPlannedJourneyOrPath rdf:type owl:ObjectProperty ;
                                   rdfs:subPropertyOf dul:realizes ;
                                   rdfs:domain tafp5:ExecutedJourney ;
                                   rdfs:range tafp5:PlannedJourneyOrPath ;
                                   rdfs:comment "Planned journey (the object) may be a path, a planned journey (in the strict sense) or a forecast journey (an updated planned journey)"@en ;
                                   rdfs:label "realizesPlannedJourneyOrPath"@en .


###  https://cdm.ovh/taf/tafp5#referenceOTN
tafp5:referenceOTN rdf:type owl:ObjectProperty ,
                            owl:FunctionalProperty ;
                   rdfs:domain tafp5:TrainRun ;
                   rdfs:range tafp5:ReferenceOTN ;
                   rdfs:label "has reference OTN" .


###  https://cdm.ovh/taf/tafp5#revisesDescriptionVersion
tafp5:revisesDescriptionVersion rdf:type owl:ObjectProperty ,
                                         owl:TransitiveProperty ;
                                rdfs:comment "Links a description version to all previous description versions (therefore not a functional property). Transitive closure of previousDescriptionVersion."@en ;
                                rdfs:label "revises description version" .


###  https://cdm.ovh/taf/tafp5#trainOperatingRU
tafp5:trainOperatingRU rdf:type owl:ObjectProperty ,
                                owl:FunctionalProperty ;
                       rdfs:domain tafp5:TrainRun ;
                       rdfs:range tafp5:TrainOperator ;
                       rdfs:comment "Points to the \"lead RU\" for the entire train"@en ;
                       rdfs:label "train operating RU"@en ;
                       rdfs:seeAlso "http://data.europa.eu/949/TrainOperatingRU"^^xsd:anyURI .


#################################################################
#    Data properties
#################################################################

###  http://www.w3.org/2006/time#inXSDDateTime
time:inXSDDateTime rdf:type owl:DatatypeProperty ;
                   rdfs:domain time:Instant ;
                   rdfs:range xsd:dateTimeStamp ;
                   rdfs:comment "not a functional property, in order to allow different time representations. Consistency between these is user responsibility."@en ;
                   rdfs:label "in XSD date time"@en .


###  http://www.w3.org/ns/prov#generatedAtTime
prov:generatedAtTime rdf:type owl:DatatypeProperty ;
                     rdfs:domain prov:Entity .


###  http://www.w3.org/ns/prov#invalidatedAtTime
prov:invalidatedAtTime rdf:type owl:DatatypeProperty .


###  https://cdm.ovh/taf/tafp5#createdAt
tafp5:createdAt rdf:type owl:DatatypeProperty ;
                rdfs:subPropertyOf prov:generatedAtTime ;
                rdfs:domain tafp5:VersionedDescription ;
                rdfs:range xsd:dateTimeStamp ;
                rdfs:comment "like other time properties, it is not made functional, to allow multiple time representations (time zones)."@en ;
                rdfs:label "created at"@en .


###  https://cdm.ovh/taf/tafp5#effectiveFrom
tafp5:effectiveFrom rdf:type owl:DatatypeProperty ;
                    rdfs:domain tafp5:VersionedDescription ;
                    rdfs:range xsd:dateTimeStamp ;
                    rdfs:label "effective from"@en .


###  https://cdm.ovh/taf/tafp5#effectiveUntil
tafp5:effectiveUntil rdf:type owl:DatatypeProperty ;
                     rdfs:subPropertyOf prov:invalidatedAtTime ;
                     rdfs:domain tafp5:VersionedDescription ;
                     rdfs:range xsd:dateTimeStamp ;
                     rdfs:comment "Value can be computed by looking up updates and their \"effective from\" datetimes, supposing the newer versions invalidate earlier ones at the time they become effective. The ontology does not go that far (it would be a matter of SWRL rules)."@en ;
                     rdfs:label "effective until"@en .


###  https://cdm.ovh/taf/tafp5#hasArrivalTime
tafp5:hasArrivalTime rdf:type owl:DatatypeProperty ,
                              owl:FunctionalProperty ;
                     rdfs:domain tafp5:JourneySchedule ;
                     rdfs:range xsd:time ;
                     rdfs:label "has arrival time"@en .


###  https://cdm.ovh/taf/tafp5#hasBICCcode
tafp5:hasBICCcode rdf:type owl:DatatypeProperty ,
                           owl:FunctionalProperty ;
                  rdfs:domain tafp5:ITU ;
                  rdfs:range xsd:string ;
                  rdfs:comment "BIC code according to ISO6346 (4 letters, 6 numeric digits and 1 check digit)"@en ;
                  rdfs:label "has BIC code" .


###  https://cdm.ovh/taf/tafp5#hasComment
tafp5:hasComment rdf:type owl:DatatypeProperty ;
                 rdfs:domain tafp5:Message ;
                 rdfs:range xsd:string ;
                 rdfs:label "has comment"@en .


###  https://cdm.ovh/taf/tafp5#hasDepartureTime
tafp5:hasDepartureTime rdf:type owl:DatatypeProperty ,
                                owl:FunctionalProperty ;
                       rdfs:domain tafp5:JourneySchedule ;
                       rdfs:range xsd:time ;
                       rdfs:label "has departure time"@en .


###  https://cdm.ovh/taf/tafp5#hasDesignation
tafp5:hasDesignation rdf:type owl:DatatypeProperty ;
                     rdfs:domain tafp5:Track ;
                     rdfs:range xsd:string ;
                     rdfs:comment "Designation: to be discussed. Probably not unique. Can have short or long forms (e.g. \"Track 5\"  in the context of some yard; \"Alnabru yard track 5\" in a larger context)."@en ;
                     rdfs:label "has designation"@en .


###  https://cdm.ovh/taf/tafp5#hasFullText
tafp5:hasFullText rdf:type owl:DatatypeProperty ,
                           owl:FunctionalProperty ;
                  rdfs:domain tafp5:Message ;
                  rdfs:range xsd:string ;
                  rdfs:comment "typically, the full XML or JSON snippet (including comments, if any)"@en ;
                  rdfs:label "has full text" .


###  https://cdm.ovh/taf/tafp5#hasMassInTons
tafp5:hasMassInTons rdf:type owl:DatatypeProperty ,
                             owl:FunctionalProperty ;
                    rdfs:domain tafp5:TrainMassState ;
                    rdfs:range xsd:decimal ;
                    rdfs:comment "provisional. To be expanded, using SSN and QUDT."@en ;
                    rdfs:label "has mass in tons"@en .


###  https://cdm.ovh/taf/tafp5#isStationary
tafp5:isStationary rdf:type owl:DatatypeProperty ,
                            owl:FunctionalProperty ;
                   rdfs:domain tafp5:TrainMotionState ;
                   rdfs:range xsd:boolean ;
                   rdfs:label "is stationary"@en .


###  https://cdm.ovh/taf/tafp5#locationName
tafp5:locationName rdf:type owl:DatatypeProperty ;
                   rdfs:domain tafp5:OperationalLocation ;
                   rdfs:range xsd:string ;
                   rdfs:label "location name"@en .


###  https://cdm.ovh/taf/tafp5#messageID
tafp5:messageID rdf:type owl:DatatypeProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain tafp5:TransportCollaborationMessage ;
                rdfs:range xsd:string ;
                rdfs:label "message ID"@en .


###  https://cdm.ovh/taf/tafp5#messageVersion
tafp5:messageVersion rdf:type owl:DatatypeProperty ;
                     rdfs:domain tafp5:TransportCollaborationMessage ;
                     rdfs:range xsd:string ;
                     rdfs:label "message version"@en .


###  https://cdm.ovh/taf/tafp5#reportedAt
tafp5:reportedAt rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf prov:generatedAtTime ;
                 rdf:type owl:FunctionalProperty ;
                 rdfs:domain tafp5:Message ;
                 rdfs:range xsd:dateTimeStamp ;
                 rdfs:comment "same semantics as prov:generatedAtTime"@en ;
                 rdfs:label "reported at"@en .


###  https://cdm.ovh/taf/tafp5#reportedBy
tafp5:reportedBy rdf:type owl:DatatypeProperty ,
                          owl:FunctionalProperty ;
                 rdfs:domain tafp5:TransportCollaborationMessage ;
                 rdfs:range xsd:string ;
                 rdfs:comment ""@en ;
                 rdfs:label "has source"@en .


###  https://cdm.ovh/taf/tafp5#sizeAndTypeCode
tafp5:sizeAndTypeCode rdf:type owl:DatatypeProperty ,
                               owl:FunctionalProperty ;
                      rdfs:domain tafp5:Container ;
                      rdfs:range xsd:string ;
                      rdfs:comment "4 digit alphanum code for container size and type, according to ISO6346"@en ;
                      rdfs:label "size and type code" .


###  https://cdm.ovh/taf/tafp5#trainDepartureDate
tafp5:trainDepartureDate rdf:type owl:DatatypeProperty ,
                                  owl:FunctionalProperty ;
                         rdfs:domain tafp5:TrainRun ;
                         rdfs:range xsd:date ;
                         rdfs:label "train departure date"@en ;
                         rdfs:seeAlso taf:TrainDepartureDate .


###  https://cdm.ovh/taf/tafp5#uicWagonNumber
tafp5:uicWagonNumber rdf:type owl:DatatypeProperty ,
                              owl:FunctionalProperty ;
                     rdfs:domain tafp5:Wagon ;
                     rdfs:range xsd:string ;
                     rdfs:comment "may it vary in a wagon's lifetime? if not, consider moving to rolling stock ontology."@en ;
                     rdfs:label "uic wagon number"@en .


#################################################################
#    Classes
#################################################################

###  http://cdm.ovh/rsm/rolling_stock/typology/typology#RollingStock
rst_typ:RollingStock rdf:type owl:Class .


###  http://cdm.ovh/rsm/rolling_stock/typology/typology#Wagon
rst_typ:Wagon rdf:type owl:Class .


###  http://www.ontologydesignpatterns.org/cp/owl/list.owl#List
list:List rdf:type owl:Class .


###  http://www.ontologydesignpatterns.org/cp/owl/list.owl#ListItem
list:ListItem rdf:type owl:Class .


###  http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Description
dul:Description rdf:type owl:Class .


###  http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#DesignedArtifact
dul:DesignedArtifact rdf:type owl:Class .


###  http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event
dul:Event rdf:type owl:Class .


###  http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#InformationObject
dul:InformationObject rdf:type owl:Class .


###  http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Organization
dul:Organization rdf:type owl:Class .


###  http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Place
dul:Place rdf:type owl:Class .


###  http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Plan
dul:Plan rdf:type owl:Class .


###  http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Process
dul:Process rdf:type owl:Class .


###  http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Role
dul:Role rdf:type owl:Class ;
         rdfs:comment "Note: aligning with DOLCE, roles are defined as individuals such as \"freight role\" (instance of Load role), with no properties attached."@en .


###  http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Situation
dul:Situation rdf:type owl:Class .


